   } else {\n        delay = 1\n    }\n    showScroll()\n    basic.showNumber(delay)\n    clear(1)\n})\nfunction scrollFall () {\n    if (Scroll != 0 || PressH) {\n        if (PressH) {\n            PressH = false\n        } else {\n            Scroll = 0\n        }\n    } else {\n        Speed = 0\n        //修正開機時位於原點上按下click導致後續使用Mouse Scroll無法作用問題\n        if (defaultMove){\n            mouse.click()\n        }\n    }\n    music.play(music.tonePlayable(523, music.beat(BeatFraction.Sixteenth)), music.PlaybackMode.InBackground)\n    basic.clearScreen()\n}\nfunction resetStatus(){\n    fornum = 0\n    Speed = 0\n    isLongTag = false\n    PressH = false\n    music.stopAllSounds()\n}\nfunction resetStatus2(){\n    fornum2 = 0\n    PressH = false\n    Scroll = 0\n    Btm = 0\n    BtmP = false\n    inhaleTag = false\n    music.stopAllSounds()\n}\nlet MoveY = 0\nlet MoveX = 0\nlet Btm = 0\nlet inhaleTag = false //吸氣判定\nlet BtmP = false\nlet DirY = 0\nlet DirX = 0\nlet Cur = 0\nlet CurP = false\nlet isLongTag = false //長吹判定\nlet saveTime = 0 //紀錄時間\nlet fornum = 0 //吐氣時的次數計算\nlet fornum2 = 0 //吸氣時的次數計算\nlet PaValue = 0 //壓力數值\nlet nowTime = 0 //開始時間\nlet PressH = false\nlet Scroll = 0\nlet delay = 0\nlet SpeedMem = 0\nlet Speed = 0\nlet Sound = 0\nlet showtime = 0\nlet defaultTime = 1000 // 判定時間1秒\nlet defaultMove = false //用來判斷是否有移動\nlet defaultMinPaValue = -10000093 //校正時調整defaultMinPaValue或defaultMaxPaValue，校正PaValue至-93\nlet defaultMaxPaValue = 10000000 //校正時調整defaultMinPaValue或defaultMaxPaValue，校正PaValue至-93\nlet holdNum = 1\nmouse.startMouseService()\nshowtime = 50\nSound = 5\nSpeed = 5\nSpeedMem = Speed\ndelay = 5\nlet lastScroll = 1\nlet basicScroll = 1 //避免Scroll及lastScroll同時為0時重新給1\nmusic.play(music.builtinPlayableSoundEffect(soundExpression.giggle), music.PlaybackMode.InBackground)\nHX711.SetPIN_SCK(DigitalPin.P2)\nHX711.SetPIN_DOUT(DigitalPin.P1)\nHX711.begin()\n//檢查偏差值\nlet offsetPaValue = HX711.read()\n//檢查偏差值\nfor (let index = 0; index < 2; index++) {\n   